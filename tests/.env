# the pool of master nodes pool
DB_NAME=default.master
DB_URI=127.0.0.1:3306/test?user=root&password=&charset=utf8
DB_MIN_ACTIVE=1
DB_MAX_ACTIVE=60
DB_MAX_WAIT=10
DB_MAX_WAIT_TIME=10
DB_MAX_IDLE_TIME=60
DB_TIMEOUT=2
DB_USE_PROVIDER=false
DB_BALANCER=random
DB_PROVIDER=consul2

# the pool of slave nodes pool
DB_SLAVE_NAME=default.slave
DB_SLAVE_URI=127.0.0.1:3306/test?user=root&password=&charset=utf8
DB_SLAVE_MIN_ACTIVE=1
DB_SLAVE_MAX_ACTIVE=60
DB_SLAVE_MAX_WAIT=10
DB_SLAVE_MAX_WAIT_TIME=10
DB_SLAVE_MAX_IDLE_TIME=60
DB_SLAVE_TIMEOUT=3
DB_SLAVE_USE_PROVIDER=false
DB_SLAVE_BALANCER=random
DB_SLAVE_PROVIDER=consul2

